<main class="container">
    <section class="login_header page_header">
        <h1>Log In Page</h1>
    </section>
    <section class="login_body page_body">
        <%= form_with(model: @user, url: "/login") do |f| %>
            <%= f.label :username %><br>
            <%= f.text_field :username %><br><br>
            <%= f.label :password %><br>
            <%= f.text_field :password %><br><br>
            <%= f.submit "Log In" %>
        <% end %>
    </section>
</main>