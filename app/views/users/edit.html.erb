<main class="container">
    <section class="edit_user_header">
        <h1>EDIT USER SIGNUP</h1>
    </section>

    <section class="edit_user_body">
        <%= form_for @user do |f| %>
            <%= f.label :username %><br>
            <%= f.text_field :username %><br><br>
            <%= f.label :email %><br>
            <%= f.text_field :email %><br><br>
            <%= f.submit "EDIT ACCOUNT" %>
        <% end %>
    </section>
</main>